{
    "name": "shahadh/laravel-temporary-links",
    "description": "Generate temporary, expiring access links for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Shahadh",
            "email": "shahadh.hamza@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Shahadh\\TemporaryLinks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shahadh\\TemporaryLinks\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shahadh\\TemporaryLinks\\TemporaryLinksServiceProvider"
            ],
            "aliases": {
                "TemporaryLink": "Shahadh\\TemporaryLinks\\Facades\\TemporaryLink"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
